    "build": "cross-env NODE_ENV=production webpack --config conf/webpack.production.js",
    "build:gh": "npm run build && node ./build-gh-pages.js && rm -r ./dist",
    "deploy:surge": "npm build && surge -p build --domain animatedpotato.surge.sh"
